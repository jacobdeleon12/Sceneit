{
  "name": "mern",
  "version": "1.0.0",
  "description": "Proposal for Project #3\r (Blake Marter, Jacob Deleon, Kai Richardson, Kaleb Merriman)",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7",
    "prettier": "^1.17.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "moment": "^2.24.0",
    "mongoose": "^5.3.16",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "react-google-login": "^5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacobdeleon12/Sceneit.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/jacobdeleon12/Sceneit/issues"
  },
  "homepage": "https://sceneitapp.herokuapp.com/"
}
